{
  "manifest_version": 3,
  "name": "Linkedin Filter",
  "version": "1.0",
  "description": "Linkedin Filter",
  "action": {
    "default_popup": "index.html",
    "default_title": "Linkedin Filter"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.linkedin.com/*"]
    }
  ],
  "permissions": ["activeTab", "storage", "tabs", "unlimitedStorage"]
}
